<form class="rolenroll-sheet actor-sheet" autocomplete="off">
  <!-- ================= HEADER ================= -->
  <header class="sheet-header flexrow rr-header">
    <!-- ICON -->
    <img class="profile-img rr-icon"
         src="{{actor.img}}"
         data-edit="img"
         title="{{actor.name}}"/>

    <!-- RIGHT SIDE (everything else) -->
    <div class="rr-header-main">

      <!-- NAME BAR -->
      <div class="rr-name-row">
        <input name="name"
               class="rr-name-input"
               type="text"
               value="{{actor.name}}"
               placeholder="Character Name"/>
      </div>

      <!-- STATS + MENTAL ROW -->
      <div class="rr-stats-row">

        <!-- LEFT COLUMN: Health / Defense / Will Power -->
        <div class="rr-stats-left">
          <div class="rr-stat-line">
            <label>Health</label>
            <input type="number" name="system.hpCurrent" value="{{system.hpCurrent}}" min="0"/>
            <span class="rr-slash">/</span>
            <input type="number" name="system.hpMax" value="{{system.hpMax}}" min="0"/>
          </div>

          <div class="rr-stat-line">
            <label>Defense</label>
            <input type="number"
                   name="system.defense"
                   value="{{system.defense}}"
                   min="0"
                   class="rr-defense-input"/>
          </div>

          <div class="rr-stat-line">
            <label>Will Power</label>
            <input type="number" name="system.wpCurrent" value="{{system.wpCurrent}}" min="0"/>
            <span class="rr-slash">/</span>
            <input type="number" name="system.wpMax" value="{{system.wpMax}}" min="0"/>
          </div>
        </div>

        <!-- MIDDLE COLUMN: LV / EXP -->
        <div class="rr-stats-middle">
          <div class="rr-small-box">
            <label>LV.</label>
            <input type="number" name="system.level" value="{{system.level}}" min="0"/>
          </div>
          <div class="rr-small-box">
            <label>EXP.</label>
            <input type="number" name="system.exp" value="{{system.exp}}" min="0"/>
          </div>
        </div>

        <!-- RIGHT COLUMN: Mental hearts -->
        <div class="rr-mental-wrapper">
          <div class="rr-mental-label">Mental</div>
          <div class="rr-mental-hearts" data-current="{{system.mentalHearts}}">
            {{!-- 18 hearts, index 0–17 --}}
            <span class="rr-heart" data-index="0">♥</span>
            <span class="rr-heart" data-index="1">♥</span>
            <span class="rr-heart" data-index="2">♥</span>
            <span class="rr-heart" data-index="3">♥</span>
            <span class="rr-heart" data-index="4">♥</span>
            <span class="rr-heart" data-index="5">♥</span>
            <span class="rr-heart" data-index="6">♥</span>
            <span class="rr-heart" data-index="7">♥</span>
            <span class="rr-heart" data-index="8">♥</span>
            <span class="rr-heart" data-index="9">♥</span>
            <span class="rr-heart" data-index="10">♥</span>
            <span class="rr-heart" data-index="11">♥</span>
            <span class="rr-heart" data-index="12">♥</span>
            <span class="rr-heart" data-index="13">♥</span>
            <span class="rr-heart" data-index="14">♥</span>
            <span class="rr-heart" data-index="15">♥</span>
            <span class="rr-heart" data-index="16">♥</span>
            <span class="rr-heart" data-index="17">♥</span>
          </div>
        </div>

      </div> <!-- end rr-stats-row -->

    </div> <!-- end rr-header-main -->
  </header>

  <!-- =============== TABS NAV =============== -->
  <nav class="sheet-tabs tabs" data-group="primary">
    <a class="item active" data-tab="attribute">Attribute</a>
    <a class="item" data-tab="general">General Ability</a>
  </nav>

